{
  "name": "jscpd",
  "version": "0.3.4",
  "description": "Copy/paste detector for programming code, support JavaScript, CoffeeScript, PHP, Ruby, Python, Less, Go, Java, C#, C++, C languages",
  "author": {
    "name": "Andrey Kucherenko",
    "email": "kucherenko.andrey@gmail.com",
    "url": "http://kucherenko.org/"
  },
  "main": "index.js",
  "bin": {
    "jscpd": "./bin/jscpd"
  },
  "dependencies": {
    "shelljs": "*",
    "coffee-script": "*",
    "crypto": "*",
    "cli": "*",
    "js-yaml": "*",
    "glob": "*",
    "underscore": "*",
    "codemirror": "*",
    "winston": "*"
  },
  "devDependencies": {
    "mocha": "*",
    "jscpd": "*",
    "chai": "*",
    "sinon": "*",
    "xml2js": "*",
    "coffee-coverage": "*",
    "mocha-phantom-coverage-reporter": "*",
    "coveralls": "*"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register -R spec $(find test -name '*.test.coffee')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kucherenko/jscpd.git"
  },
  "keywords": [
    "code",
    "cpd",
    "pmd",
    "analyze",
    "quality",
    "copy",
    "paste",
    "javascript",
    "php",
    "python",
    "coffeescript",
    "ruby",
    "java",
    "c#",
    "c++",
    "c"
  ],
  "readmeFilename": "README.md",
  "licenses": [
    {
        "type": "MIT",
        "url": "https://github.com/kucherenko/jscpd/blob/master/LICENSE"
    }
  ],
  "preferGlobal": true,
  "engines": {
    "node": "~0.10.x"
  }
}
