  "dependencies": {
    "shelljs": ">=0.5.x",
    "crypto": ">=0.0.x",
    "cli": ">=0.10.x",
    "js-yaml": ">=3.4.x",
    "glob": ">=5.0.x",
    "underscore": ">=1.8.x",
    "codemirror": ">=4.10.x",
    "winston": ">=1.0.x"
  },
  "devDependencies": {
    "chai": ">=1.10.x",
    "coffee-coverage": ">=0.4.4",
    "coffee-script": ">=1.10.x",
    "coveralls": "2.11.x",
